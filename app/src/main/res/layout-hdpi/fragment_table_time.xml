<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="70dp"
    android:orientation="vertical"
    tools:context="com.example.tiagotoscano.freetec.TableTimeActivityFragment"
    tools:showIn="@layout/activity_table_time">

    <GridView
        android:layout_width="wrap_content"
        android:layout_height="85dp"
        android:columnWidth="80dp"
        android:numColumns="auto_fit"
        android:verticalSpacing="0dp"

        android:horizontalSpacing="0dp"
        android:stretchMode="columnWidth"
        android:gravity="top"
        android:id="@+id/gridView"

        />
    <ListView
        android:layout_marginTop="75dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:id="@+id/listView"
        android:layout_gravity="top" />
    <FrameLayout
        android:visibility="invisible"
        android:background="@color/greyAlpha"
        android:layout_width="match_parent"
        android:id="@+id/framAlert"
        android:layout_height="match_parent"
        android:layout_gravity="center">

    <FrameLayout
        android:visibility="visible"
        android:background="@drawable/screen_border"
        android:layout_width="match_parent"
        android:id="@+id/framAlertWin"
        android:layout_height="220dp"
        android:layout_gravity="center">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fechar X"
                android:layout_marginTop="5dp"
                android:layout_marginRight="8dp"
                android:onClick=""
                android:id="@+id/bntClose"
                android:layout_gravity="right" />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal">
                <TextView
                    android:paddingLeft="16dp"
                    android:layout_width="72dp"
                    android:layout_height="wrap_content"
                    android:text="Curso:"
                    android:id="@+id/textView6"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="230dp"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/labCursodesc"
                    android:layout_gravity="center_horizontal"
                    android:textColor="#000000" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:orientation="horizontal">
                <TextView
                    android:paddingLeft="16dp"
                    android:layout_width="72dp"
                    android:layout_height="wrap_content"
                    android:text="HorÃ¡rio:"
                    android:id="@+id/textView7"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="230dp"
                    android:layout_height="wrap_content"
                    android:text="New Text"
                    android:id="@+id/labHorariodesc"
                    android:layout_gravity="center_horizontal"
                    android:textColor="#000000" />

            </LinearLayout>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Inscrever-se"
                android:id="@+id/bntInscricao"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:paddingTop="4dp"
                android:layout_width="329dp"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Medium Text"
                android:id="@+id/labMsgRetorno"
                android:visibility="invisible"
                android:layout_gravity="center_horizontal"
                android:textColor="#ce0808" />


        </LinearLayout>



    </FrameLayout>
    </FrameLayout>

</FrameLayout>
